<%- include('../header.ejs') %>

<br><br><br><br>
<div class ="container">
<div class="content mt-5">
    <div class="animated fadeIn">
      <div class="row">
        <div class="col-md-12">
          <div class="card">
            <div class="card-header d-flex justify-content-between">
              <div>
                <strong class="card-title text-success"
                  >Student More Details</strong
                >
              </div>
            </div>
            <div class="card-body">
              <table
                id="bootstrap-data-table-export"
                class="table table-striped table-bordered"
              >
                <thead class="text-white bg-success">
                  <tr>
                    <th>Name</th>
                    <th>Gender</th>
                    <th>Email</th>
                    <th>Mobile</th>
                    <th>Course</th>
                    <th>Dob</th>
                    <th>Address</th>
                    <th>College</th>
                    <th>Branch</th>
                     
                  </tr>
                </thead>
                <tbody>
                  
                  <tr>
                    <td><%= c.name %></td>
                    <td><%= c.gender %></td>
                    <td><%= c.email %></td>
                    <td><%= c.mobile %></td>
                    <td><%= c.subject %></td>
                    <td><%= c.dob %></td>
                    <td><%= c.address %></td>
                    <td><%= c.collage %></td>
                    <td><%= c.branch %></td>
                    
                      
                    
                </tr>
  
                
               
              </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
      <!-- row end-->
    </div>
  </div>
  </div>
  <!-- container fluid end-->
  <br /><br />