<%- include('../header.ejs') %>
<br><br><br><br>
<div class ="container">
<div class="content mt-5">
    <div class="animated fadeIn">
      <div class="row">
        <div class="col-md-12">
          <div class="card">
            <div class="card-header d-flex justify-content-between">
              <div>
                <strong class="card-title text-success"
                  >Student Course Register Table</strong
                >
              </div>
            </div>
            <div class="card-body">
              <table
                id="bootstrap-data-table-export"
                class="table table-striped table-bordered"
              >
                <thead class="text-white bg-success">
                  <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Course</th>
                    <th>Mobile</th>
                    <th>Action</th>
                   
                  </tr>
                </thead>
                <tbody>
                  <% c.forEach((item)=>{ %>
                  <tr>
                    <td><%= item.name %></td>
                   
                    <td><%= item.email %></td>
                    <td><%= item.subject %></td>
                    <td><%= item.mobile %></td>
                      
                    <td class="text-center">
                        <a href="/admin/courseview/<%= item._id %>" class="btn btn-success"
                          >view</i
                        ></a>
                          <a href="/admin/coursedelete/<%= item._id %>" class="btn btn-danger">Delete</a>
                         
                      </td>
                </tr>
  
                
                <% }) %>
              </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
      <!-- row end-->
    </div>
  </div>
  </div>
  <!-- container fluid end-->
  <br /><br />